<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"      
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javae      
    /web-app_2_5.xsd" id="WebApp_ID" version="2.5">
   <welcome-file-list>
   		<welcome-file>Frontend.jsp</welcome-file>
   		<welcome-file>Login.jsp</welcome-file>
   		<welcome-file>PlaceOrder.jsp</welcome-file>
   </welcome-file-list>
  <display-name>InventorySystem</display-name>
  <servlet>
    <servlet-name>StockServlet</servlet-name>
    <servlet-class>com.java.servlet.StockServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StockServlet</servlet-name>
    <url-pattern>/displayStockInf.jsp</url-pattern>
  </servlet-mapping>
</web-app>